{
  "builds": [
    {
      "src": "tasty_trails/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "tasty_trails/wsgi.py"
    }
  ],
  "hooks": {
    "build": "pip install -r requirements.txt"
  },
  "env": {
    "PYTHON_VERSION": "3.9" 
  }
}